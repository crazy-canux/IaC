# Example terraform.tfvars for Cilium module
# Copy this file to terraform.tfvars and update the values

# Get these values from Kind module outputs:
# cd ../kind && terraform output -raw kubeconfig_path
kubeconfig_path = "/Users/username/.kube/config"

# cd ../kind && terraform output -raw cluster_name  
cluster_name = "iac-lab"

# Optional: Override defaults
# cilium_version = "1.17.1"
# environment = "development"
# enable_hubble_ui = true
